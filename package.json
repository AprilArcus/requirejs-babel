{
  "name": "requirejs-babel7",
  "version": "0.0.0-development",
  "description": "An AMD module loader plugin for using RequireJS with Babel 7.",
  "author": "Ferdinand Prantl <prantlf@gmail.com>",
  "contributors": [
    "Michael <mikach27@gmail.com>"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/requirejs-babel/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/requirejs-babel#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/requirejs-babel.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/requirejs-babel/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "es6.js",
  "scripts": {
    "prepare": "npm run generate && npm run optimize",
    "generate": "npm run generate:helpers && npm run minify:helpers && npm run minify:polyfill && npm run minify:requirejs",
    "generate:helpers": "babel-external-helpers -t global > demo-polyfill/babel-helpers.js",
    "minify:helpers": "terser -o demo-polyfill/babel-helpers.min.js -cm --comments false --source-map includeSources --source-map url=babel-helpers.min.js.map demo-polyfill/babel-helpers.js",
    "minify:polyfill": "terser -o demo-polyfill/babel-polyfill.min.js -cm --comments false --source-map includeSources --source-map url=babel-polyfill.min.js.map node_modules/@babel/polyfill/dist/polyfill.js",
    "minify:requirejs": "terser -o demo-polyfill/require.min.js -cm --comments false --source-map includeSources --source-map url=require.min.js.map node_modules/requirejs/require.js",
    "optimize": "r.js -o demo/build.config.js && r.js -o demo-polyfill/build.config.js",
    "test": "eslint es6.js"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "failComment": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "peerDependencies": {
    "@babel/standalone": "7.x",
    "babel-plugin-module-resolver-standalone": "0.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/polyfill": "^7.4.0",
    "@babel/standalone": "^7.16.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "babel-plugin-module-resolver-standalone": "^0.0.2",
    "connect": "^3.7.0",
    "connect-block-favicon": "^1.0.4",
    "eslint": "^8.5.0",
    "requirejs": "^2.3.6",
    "serve-static": "^1.14.2",
    "terser": "^5.10.0"
  },
  "keywords": [
    "requirejs-plugin",
    "requirejs",
    "plugin",
    "babel"
  ]
}
